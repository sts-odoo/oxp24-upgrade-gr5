<?xml version="1.0" encoding="utf-8"?>
<data>
    <odoo>
        <record id="sale_subscription_view_form" model="ir.ui.view">
            <field name="name">sale.subscription.customize.form.inherit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale_subscription.sale_subscription_primary_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='next_invoice_date']" position="before">
                    <field name="amount_at_start"/>
                    <field name="parent_id"/>
                    <field name="child_recurring_total"/>
                </xpath>
            </field>
        </record>

    </odoo>
</data>
